// Find & Unlike all likes
window.setInterval(function() {
	var elements = document.querySelectorAll('[role="button"][data-testid="unlike"]');
	for (var i=0;i<elements.length;i++) {
	   elements[i].click();
	}
},2000);

// Keep scrolling down
window.setInterval(function() {
	window.scrollTo(0,document.body.scrollHeight);
},1000);
