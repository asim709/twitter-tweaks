// Open Retweet Menu
window.setInterval(function() {
	var element = document.querySelector('div[role="button"][data-testid="unretweet"]');
	if (element) {
		element.click();
	}	
},1000);

// Click Undo retweet menu item
window.setInterval(function() {
	var element = document.querySelector('div[role="menuitem"][data-testid="unretweetConfirm"]');
	if (element) {
		console.log(element);
		element.click();
	}	
},500);
